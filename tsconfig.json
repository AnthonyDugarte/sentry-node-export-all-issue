{
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"ES2022",
			"DOM"
		],
		"module": "Node16",
		"outDir": "./dist",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2022",
		"disableSourceOfProjectReferenceRedirect": true
	}
}
